{
  "name": "@undermuz/use-form",
  "version": "1.0.1",
  "description": "React library for build forms",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/types/index.d.js",
  "typings": "build/types/index.d.js",
  "sideEffects": false,
  "files": [
    "build/cjs/",
    "build/esm/",
    "build/types/"
  ],
  "scripts": {
    "test": "jest",
    "build": "node tools/cleanup && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p config/tsconfig.cjs.json",
    "build:esm": "tsc -p config/tsconfig.esm.json",
    "build:types": "tsc -p config/tsconfig.types.json",
    "build:umd": "webpack --config config/webpack.config.js",
    "clean": "node tools/cleanup"
  },
  "author": {
    "name": "undermuz",
    "email": "undermuz@gmail.com",
    "url": "https://github.com/undermuz"
  },
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "12.0.0",
    "@testing-library/react-hooks": "7.0.1",
    "@testing-library/user-event": "13.1.9",
    "@types/react": "^18.0.5",
    "babel-jest": "^28.1.1",
    "eslint": "^8.9.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^28.1.1",
    "jest-cli": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "keywords": [
    "react",
    "form",
    "forms",
    "use-form",
    "hook"
