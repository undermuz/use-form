{
  "name": "@undermuz/use-form",
  "version": "1.0.16",
  "description": "React library for build forms",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "node": "./dist/index.cjs",
    "require": "./dist/index.cjs",
    "default": "./dist/index.cjs"
  },
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint",
    "build": "npm run build:tsup",
    "build:tsup": "tsup",
    "build:old": "node tools/cleanup && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p config/tsconfig.cjs.json",
    "build:esm": "tsc -p config/tsconfig.esm.json",
    "build:types": "tsc -p config/tsconfig.types.json",
    "build:umd": "webpack --config config/webpack.config.js",
    "clean": "node tools/cleanup"
  },
  "author": {
    "name": "undermuz",
    "email": "undermuz@gmail.com",
    "url": "https://github.com/undermuz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/undermuz/use-form.git"
  },
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.3.1",
    "underscore": "^1.13.4"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "12.0.0",
    "@testing-library/react-hooks": "7.0.1",
    "@testing-library/user-event": "13.1.9",
    "@types/react": "^18.0.5",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "babel-jest": "^28.1.1",
    "esbuild-plugin-file-path-extensions": "^1.0.0",
    "eslint": "^8.9.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^28.1.1",
    "jest-cli": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "ts-loader": "^9.2.8",
    "tsup": "^6.6.3",
    "typescript": "^4.5.5",
    "typescript-eslint": "^0.0.1-alpha.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "keywords": [
    "react",
    "form",
    "forms",
    "use-form",
    "hook"
  ]
}
